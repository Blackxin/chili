#!/usr/bin/env bash

seekascan()
{
   local n=$(($#-1))
   local value=${!n}

array=("${@:1}")
unset array[$n]
echo "${array[@]}"
echo "tamanho: $n"
echo "search : $value"
echo "search: ${!n}"
declare -p array

   for ((i=1;i < $#;i++)) {
      if [ "${!i}" == "${value}" ]; then
         ((--i)); echo $i
         return 0
      fi
   }
   return 1
}

arr=({a..z})
search=c
seekascan "${arr[@]}" $search
exit

if value=$(seekascan "${arr[@]}" $search); then
	echo $value
else
	echo nÃ£o encontrado
fi

